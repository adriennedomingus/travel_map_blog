<div class="container">
  <h1 class="centered"> Your Photos </h1>
  <% if @photos.length == 0 %>
    <p class="centered">You haven't uploaded any photos yet!</p>
  <% else %>
    <% @photos.each do |photo| %>
      <div class="col-md-4">
        <%= render partial: "partials/photo", locals: { photo: photo } %>
      </div>
    <% end %>
  <% end %>
</div>
